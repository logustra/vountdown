{
  "name": "@logustra/vountdown",
  "version": "0.1.6",
  "description": "A minimalist countdown component for vue.js 2-3",
  "keywords": [
    "vue",
    "vue3",
    "countdown",
    "timer",
    "component",
    "ui",
    "frontend"
  ],
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.esm.js",
  "unpkg": "./dist/index.umd.js",
  "jsdelivr": "./dist/index.umd.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/logustra/vountdown"
  },
  "bugs": "https://github.com/logustra/vountdown/issues",
  "author": "Faizal Andyka",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "clean:dist": "rimraf dist",
    "clean:coverage": "rimraf coverage",
    "dev": "vite",
    "build": "pnpm clean:dist && vite build && vue-tsc --emitDeclarationOnly",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "pnpm clean:coverage && vitest --coverage"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.4.9",
    "vue": "^2.0.0 || >=3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.18.9",
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/node": "^17.0.21",
    "@vitejs/plugin-vue": "^2.2.0",
    "c8": "^7.11.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.11.0",
    "happy-dom": "^2.49.0",
    "husky": "^7.0.4",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.4",
    "vite": "^2.8.0",
    "vitest": "^0.7.6",
    "vue": "^3.2.31",
    "vue-tsc": "^0.29.8"
  },
  "dependencies": {
    "vue-demi": "^0.12.1"
  },
  "volta": {
    "node": "14.19.1"
  }
}