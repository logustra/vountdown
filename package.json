{
  "name": "@logustra/vountdown",
  "version": "0.0.6",
  "description": "A minimalist countdown component for vue.js",
  "files": [
    "dist"
  ],
  "main": "./dist/vountdown.umd.js",
  "module": "./dist/vountdown.es.js",
  "exports": {
    ".": {
      "import": "./dist/vountdown.es.js",
      "require": "./dist/vountdown.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --emitDeclarationOnly && vite build",
    "preview": "vite preview"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.4.9",
    "vue": "^2.0.0 || >=3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^17.0.21",
    "@vitejs/plugin-vue": "^2.2.0",
    "typescript": "^4.5.4",
    "vite": "^2.8.0",
    "vue": "^3.2.31",
    "vue-tsc": "^0.29.8"
  },
  "dependencies": {
    "vue-demi": "^0.12.1"
  },
  "volta": {
    "node": "14.17.1"
  }
}